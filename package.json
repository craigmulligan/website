{
  "name": "hobochild.tk",
  "version": "1.0.0",
  "scripts": {
    "build": "mdown -i'./pages/**/*.md' -o ./out --header ./assets/header.html",
    "serve": "serve ./_pages -p 4000",
    "deploy:prep":
      "cp keybase.txt out/keybase.txt && cp .nojekyll out/.nojekyll && cp CNAME out/CNAME",
    "deploy": "yarn deploy:prep && gh-pages -d out --dotfiles"
  },
  "author": "hobochild",
  "license": "ISC",
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "serve": "^11.2.0",
    "markdown-folder-to-html": "^2.3.0"
  }
}
